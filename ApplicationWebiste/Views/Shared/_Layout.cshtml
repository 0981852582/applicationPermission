<script>
    $(document).ready(function () {
        $('.sorting').click(function () {
            var existsClassASC = $(this).hasClass('sorting_asc');
            var existsClassDESC = $(this).hasClass('sorting_desc');
            $('.sorting_asc').addClass('.sorting').removeClass('.sorting_asc');
            $('.sorting_desc').addClass('.sorting').removeClass('.sorting_desc');
            if (existsClassASC) {
                $(this).addClass('sorting_desc');
            } else if (existsClassDESC) {
                $(this).addClass('sorting_asc');
            } else {
                $(this).addClass('sorting_asc');
            }
        });
        var effectOrderByHeaderTable = function ($this) {
            var existsClassASC = $this.hasClass('sorting_asc');
            var existsClassDESC = $this.hasClass('sorting_desc');
            $('.sorting_asc').addClass('sorting').removeClass('sorting_asc');
            $('.sorting_desc').addClass('sorting').removeClass('sorting_desc');

            $this.removeClass('sorting');
            if (existsClassASC) {
                $this.addClass('sorting_desc');
            } else if (existsClassDESC) {
                $this.addClass('sorting_asc');
            } else {
                $this.addClass('sorting_asc');
            }
        }
        $(document).on('click', '.sorting', function () {
            effectOrderByHeaderTable($(this));
        });
        $(document).on('click', '.sorting_asc', function () {
            effectOrderByHeaderTable($(this));
        });
        $(document).on('click', '.sorting_desc', function () {
            effectOrderByHeaderTable($(this));
        });
    });
</script>
