    <script>
        var idOfDialog = "#modal-body";
        var idOfDataTable = "#dataTable";
        var grobal_permission = undefined;
        var http;
        var timeout;
        var interval;
        var toasterCustom;
        var app = angular.module('app-module', ['ui.bootstrap', 'toaster', 'ui.bootstrap.contextMenu', 'ui.select']);
        app.controller('viewMenuPermission', function ($scope, $rootScope, $timeout, $interval, $http, toaster) {
            // khai báo những biết sử dụng chung trong tất cả function
            //      khai báo biến chứa mảng danh sách sử dụng
            $rootScope.listStatus = [
                { Id: 1, Title: 'Sử dụng' },
                { Id: 2, Title: 'Không sử dụng' }
            ];
            //      khai báo biến quy định thời gian chạy sau khi nhập tìm kiếm
            $rootScope.timeDelaySearch = 500;
            // thực hiện lấy ra url của page hiện tại để biết đang ở page nào hover vào menu bên trái.
            $rootScope.urlPageCurrent = window.location.pathname;
            //      hàm dùng chung thông ra message là 'success'
            $rootScope.showMessageSuccess = function (content, timeShow) {
                toaster.pop("success", "", content, isNull(timeShow) ? 1000 : timeShow, null);
            }
            //      hàm dùng chung thông ra message là 'error'
            $rootScope.showMessageError = showMessageError;
            $rootScope.showMessageWarning = function (content, timeShow) {
                toaster.pop("Warning", "", content, isNull(timeShow) ? 1000 : timeShow, null);
            }
            // hàm bật blockUI lên giao diện khi loading
            $rootScope.onBlockUI = function (element, message) {
                $timeout(function () {
                    App.blockUI({
                        target: element, boxed: !0, message: message
                    });
                }, 1);
            }
            // hàm tất blockUI trên giao diện khi loading
            $rootScope.offBlockUI = function (element) {
                $timeout(function () {
                    App.unblockUI(element);
                }, 1);
            }
            try {
                // lấy giá trị của ô input hidden có chứa các quyền của page này.
                $rootScope.global_permission = JSON.parse(angular.element("input[id='inputHavePermission']").val());
                // thực hiện gán giá trị đối tượng khi tìm thấy quyền truy cập trên page này .
                var convertToObject = {};
                for (var i = 0; i < $rootScope.global_permission.length; i++) {
                    convertToObject[$rootScope.global_permission[i]] = true;
                }
                // lấy ra danh sách quyền trên page này.
                $rootScope.global_permission = convertToObject;
            } catch (ex) {
                // check xem có quyền truy cập vào trang không .
                //      nếu không có thì hiện thông báo này.
                console.log('error when parse JSON array permission.');
            }
            // khai báo biến gán cho các object hoặc service của angularjs
            var trong = $scope;
            timeout = $timeout;
            interval = $interval;
            http = $http;
            toasterCustom = toaster;
            // object thực hiện lựu lại giá trị dùng chung cho page phân trang
            $rootScope.settingOfPadding = {
                // mặc định khi khởi tạo thì currentPage = 1 khi người dùng click page khác thì sử lý khác
                currentPage: 1,
                // khi số lượng page phần trang nhiều hơn maxSize thì sẽ hiện ... chứ không show hết số page phân trang
                maxSize: 5,
                // số lượng item hiện có của list để thực hiện tính toán phân ra số page
                totalItem: 0,
                // số lượng item trên 1 page để thực hiện tính toán phân ra số page
                numberPage: 10,
                // xắp xếp theo cột nào
                orderBy: 'ID',
                // đang thuộc kiểu xắp xếp nào ASC hay DESC
                orderType: false,
                // đang thuộc từ bản ghi thứ tự nào
                location: '0 bản nghi'

            };
            // call lấy dữ liệu danh mục menu bên trái khi vào admin
            http.post('/Home/viewMenuPermission/').then(function (rs) {
                $rootScope.L_permission = rs.data;
            });
        });
    </script>
